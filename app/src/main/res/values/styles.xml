<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@android:color/holo_green_dark</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:colorBackground">@color/background_material_light</item>
    </style>

    <style name="light_green_toolbar" parent="AppTheme" >
        <item name="colorPrimary">?android:attr/colorPrimaryDark</item>
        <item name="colorPrimaryDark">@android:color/holo_green_light</item>
        <item name="android:statusBarColor">@android:color/holo_green_dark</item>
    </style>

</resources>
